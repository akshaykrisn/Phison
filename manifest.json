    {
        "name": "Email Scraper",
        "description": "Scrapes emails from web pages",
        "version": "1.0",
        "manifest_version": 3,
        "action": {
            "default_popup": "popup.html"
        },
        "permissions": [
            "activeTab",
            "scripting"
        ],
        "content_scripts": [
            {
                "matches": ["<all_urls>"],
                "js": ["content.js"]
            }
        ]
    }